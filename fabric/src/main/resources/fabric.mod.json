{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "KilaBash"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.lowdragmc.photon.fabric.PhotonFabric"
    ],
    "client": [
      "com.lowdragmc.photon.client.fabric.ClientProxyImpl"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "ldlib": "${ldlib_version}"
  }
}